{
  "name": "Habit-Builder",
  "version": "1.0.0",
  "description": "Final Project of Bootcamp | Habit Builder App",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aMeizing1125/Habit-Builder.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/aMeizing1125/Habit-Builder/issues"
  },
  "homepage": "https://github.com/aMeizing1125/Habit-Builder#readme",
  "dependencies": {
    "bcrypt": "^3.0.6",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^4.1.0",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "mongoose": "^5.5.15",
    "nodemon": "^1.19.1"
  }
}
